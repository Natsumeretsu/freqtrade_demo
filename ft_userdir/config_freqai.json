{
    "$schema": "https://schema.freqtrade.io/schema.json",
    "max_open_trades": 1,
    "stake_currency": "USDT",
    "stake_amount": "unlimited",
    "tradable_balance_ratio": 0.99,
    "fiat_display_currency": "USD",
    "dry_run": true,
    "dry_run_wallet": 1000,
    "cancel_open_orders_on_exit": false,
    "trading_mode": "futures",
    "timeframe": "5m",
    "user_data_dir": ".",
    "margin_mode": "isolated",
    "unfilledtimeout": {
        "entry": 10,
        "exit": 10,
        "exit_timeout_count": 0,
        "unit": "minutes"
    },
    "entry_pricing": {
        "price_side": "same",
        "use_order_book": true,
        "order_book_top": 1,
        "price_last_balance": 0.0,
        "check_depth_of_market": {
            "enabled": false,
            "bids_to_ask_delta": 1
        }
    },
    "exit_pricing": {
        "price_side": "same",
        "use_order_book": true,
        "order_book_top": 1
    },
    "exchange": {
        "name": "okx",
        "key": "",
        "secret": "",
        "ccxt_config": {},
        "ccxt_async_config": {},
        "pair_whitelist": ["ETH/USDT:USDT"],
        "pair_blacklist": []
    },
    "pairlists": [
        {
            "method": "StaticPairList"
        }
    ],
    "dataformat_ohlcv": "feather",
    "dataformat_trades": "feather",
    "freqai": {
        "enabled": true,
        "purge_old_models": 2,
        "train_period_days": 60,
        "backtest_period_days": 3,
        "identifier": "eth_lgb_clf_v1",
        "continual_learning": true,
        "feature_parameters": {
            "include_timeframes": ["5m", "15m", "1h"],
            "include_corr_pairlist": [],
            "label_period_candles": 20,
            "include_shifted_candles": 2,
            "DI_threshold": 0.9,
            "weight_factor": 0.9,
            "principal_component_analysis": false,
            "use_SVM_to_remove_outliers": true,
            "svm_params": {
                "shuffle": false,
                "nu": 0.1
            },
            "use_DBSCAN_to_remove_outliers": false,
            "indicator_max_period_candles": 20,
            "indicator_periods_candles": [5, 10, 20]
        },
        "data_split_parameters": {
            "test_size": 0.25,
            "shuffle": false
        },
        "model_training_parameters": {
            "n_estimators": 1000,
            "learning_rate": 0.02,
            "max_depth": 8,
            "num_leaves": 64,
            "verbosity": -1,
            "random_state": 42
        }
    }
}
